[*]
charset=utf-8
end_of_line=lf
trim_trailing_whitespace=true
insert_final_newline=true
indent_style=space
indent_size=4

[{*.py,*.ng,*.sht,*.html,*.shtm,*.shtml,*.htm}]
indent_size=4

[{.babelrc,.stylelintrc,.eslintrc,*.json,*.jsb3,*.jsb2,*.bowerrc}]
indent_size=2

[{*.js,*.ts,*.tsx}]
indent_size=2

[*.scss]
indent_size=2

[{*.yml,*.yaml}]
indent_size=2
